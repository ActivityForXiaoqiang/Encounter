<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="ecncounter.wilson.com.encounter">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:name=".EApplication"
        android:allowBackup="true"
        android:icon="@mipmap/icon_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".EUI.Activity.MainActivity"
            android:label="@string/app_name">
        </activity>
        <activity android:name=".EUI.Activity.DetailActivity"/>
        <activity android:name=".EUI.Activity.SettingActivity"/>
        <activity android:name=".EUI.Activity.MyProposalActivity"/>
        <activity android:name=".EUI.Activity.WriteProposalActivity"/>
        <activity android:name=".EUI.Activity.SportActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".EUI.Activity.EditActivity">
        </activity>
        <activity android:name=".EUI.Activity.AccountActivity">
        </activity>
    </application>

</manifest>